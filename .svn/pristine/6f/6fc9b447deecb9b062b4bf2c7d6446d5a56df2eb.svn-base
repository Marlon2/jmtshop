@charset "utf-8";
* {
	margin:0;
	padding:0;
}
body,td,th {
	font-size: 12px;
}
body {
	margin-left: 0px;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
}
a {
	font-size: 12px;
	color: #000;
	text-decoration: none;
}
a:visited {
	color: #333;
	text-decoration: none;
}
a:hover {
	color: #000;
	text-decoration: none;
}
a:active {
	color: #000;
	text-decoration: none;
}
img {
	border:none;
}
table {
	border-collapse:collapse;
}
ul {
	list-style:none;
}
.Cal {
	clear:both;
}
#LoginLogo {
	width:256px;
	margin:0 auto;
}
#LoginTop {
	width:286px;
	margin:0 auto;
	background-image:url(../images/NavBg.jpg);
	background-position:0 -38px;
	height:37px;
}
#LoginLogo {
	height:86px;
	margin-top:30px;
}
#LoginTop {
	height:37px;
	margin-top:15px;
}
.BarLeft {
	width:8px;
	height:37px;
	background-image:url(../images/NavBg.jpg);
	float:left;
}
.BarRight {
	width:8px;
	height:37px;
	background-image:url(../images/NavBg.jpg);
	background-position:0 -76px;
	float:left;
}
#LoginTopText {
	width:270px;
	float:left;
	color:#FFF;
	font-size:16px;
	line-height:37px;
	font-weight:bold;
	text-indent:24px;
}
#LoginBox {
	width:284px;
	border:1px solid #CCC;
	border-top:none;
	margin:0 auto;
	height:180px;
}
.FontWeight {
	font-size:16px;
	font-weight:bold;
}
.Font999 {
	color:#999;
}
.userLogin {
	border:1px solid #CCC;
	height:22px;
	font-size:14px;
	line-height:22px;
}
.TextArea {
	border:1px solid #CCC;
	font-size:14px;
	line-height:22px;
}
.Button {
	height:35px;
	width:86px;
	background-image:url(../images/but.png);
	background-position:0 0;
	border:1px solid #CCC;
	font-size:14px;
	line-height:35px;
	font-weight:bold;
	color:#FFF;
}
.Buttons {
	border:1px solid #CCC;
	color:#FFF;
	background-color:#4AB902;
}
#Logo,#Nav,#Content,#Bottom {
	width:950px;
	margin:0 auto;
}

